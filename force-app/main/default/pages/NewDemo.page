<apex:page standardController="Account" recordSetVar="a">
    <apex:pageBlock >
   		<apex:pageBlockTable value="{!a}" var="acc">
            <apex:column value="{!acc.Name}"/>
            <apex:column value="{!acc.Phone}"/>
            <apex:column value="{!acc.BillingStreet}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>